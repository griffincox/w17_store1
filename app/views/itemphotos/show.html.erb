<div class="page-header">
  <h3>Itemphoto #<%= @itemphoto.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Item</dt>
      <dd>
        <% if @itemphoto.item.present? %>
          <a href="/items/<%= @itemphoto.item_id %>">
            <%= @itemphoto.item.name %>
          </a>
        <% end %>
      </dd>

      <dt>Image</dt>
      <dd><img src="<%= @itemphoto.image %>" class="img-responsive"></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/itemphotos" class="btn btn-primary">
        Back
      </a>
      <a href="/itemphotos/<%= @itemphoto.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_itemphoto/<%= @itemphoto.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
