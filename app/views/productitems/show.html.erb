<div class="page-header">
  <h3>Productitem #<%= @productitem.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Cart</dt>
      <dd><%= @productitem.cart_id %></dd>

      <dt>Order</dt>
      <dd><%= @productitem.order_id %></dd>

      <dt>Item</dt>
      <dd><%= @productitem.item_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/productitems" class="btn btn-primary">
        Back
      </a>
      <a href="/productitems/<%= @productitem.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_productitem/<%= @productitem.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
